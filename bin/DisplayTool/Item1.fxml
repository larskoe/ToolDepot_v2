<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.layout.AnchorPane?>

<AnchorPane prefHeight="350.0" prefWidth="450.0" xmlns="http://javafx.com/javafx/8.0.171" xmlns:fx="http://javafx.com/fxml/1" fx:controller="DisplayTool.MainControllerDisplay">
    <children>
      <Pane prefHeight="350.0" prefWidth="450.0">
         <children>
            <ImageView fx:id="Pic" fitHeight="200.0" fitWidth="200.0" layoutX="14.0" layoutY="37.0" pickOnBounds="true" preserveRatio="true">
               <image>
                  <Image url="@../resource/img/not-found.png" />
               </image>
            </ImageView>
            <HBox layoutX="225.0" layoutY="37.0" prefHeight="22.0" prefWidth="223.0">
               <children>
                  <Label fx:id="toolName" contentDisplay="BOTTOM" prefHeight="24.0" prefWidth="221.0" text="Name" />
               </children>
            </HBox>
            <HBox layoutX="225.0" layoutY="103.0" prefHeight="22.0" prefWidth="221.0">
               <children>
                  <Label fx:id="owner" contentDisplay="BOTTOM" prefHeight="22.0" prefWidth="224.0" text="owner" />
               </children>
            </HBox>
            <HBox layoutX="224.0" layoutY="59.0" prefHeight="22.0" prefWidth="223.0">
               <children>
                  <Label fx:id="price" prefHeight="17.0" prefWidth="217.0" text="price" />
               </children>
            </HBox>
            <HBox layoutX="224.0" layoutY="81.0" prefHeight="22.0" prefWidth="223.0">
               <children>
                  <Label fx:id="condition" contentDisplay="BOTTOM" prefHeight="24.0" prefWidth="221.0" text="Condition" />
               </children>
            </HBox>
            <Label fx:id="description" layoutX="225.0" layoutY="125.0" prefHeight="74.0" prefWidth="182.0" text="discription" />
            <Button fx:id="rent" layoutX="193.0" layoutY="294.0" mnemonicParsing="false" onAction="#rent" text="Rent" />
            <Label fx:id="avail" layoutX="225.0" layoutY="220.0" prefHeight="37.0" prefWidth="223.0" text="availability" />
         </children>
      </Pane>
   </children>
</AnchorPane>
