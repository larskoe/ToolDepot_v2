<AnchorPane xmlns="http://javafx.com/javafx/8.0.171" xmlns:fx="http://javafx.com/fxml/1" fx:controller="checkOut.MainControllerCheckOut">
   <children>
      <Pane prefHeight="450.0" prefWidth="450.0">
         <children>
            <Label layoutX="151.0" layoutY="27.0" text="Rental Cart">
               <font>
                  <Font size="31.0" />
               </font>
            </Label>
            <ImageView fx:id="Image" fitHeight="130.0" fitWidth="122.0" layoutX="14.0" layoutY="84.0" pickOnBounds="true" preserveRatio="true">
               <image>
                  <Image url="@../resource/img/not-found.png" />
               </image>
            </ImageView>
            <HBox layoutX="147.0" layoutY="84.0" prefHeight="22.0" prefWidth="122.0">
               <children>
                  <Label fx:id="Name" contentDisplay="BOTTOM" prefHeight="22.0" prefWidth="125.0" text="Name" />
               </children>
            </HBox>
            <HBox layoutX="275.0" layoutY="84.0" prefHeight="22.0" prefWidth="108.0">
               <children>
                  <Label fx:id="Price" contentDisplay="BOTTOM" prefHeight="22.0" prefWidth="100.0" text="Price" />
               </children>
            </HBox>
            <HBox layoutX="213.0" layoutY="106.0" prefHeight="22.0" prefWidth="91.0">
               <children>
                  <Label fx:id="Condition" contentDisplay="BOTTOM" prefHeight="24.0" prefWidth="221.0" text="Condition" />
               </children>
            </HBox>
            <HBox layoutX="197.0" layoutY="128.0" prefHeight="22.0" prefWidth="122.0">
               <children>
                  <Label fx:id="Renter" contentDisplay="BOTTOM" prefHeight="24.0" prefWidth="221.0" text="Renter">
                     <font>
                        <Font name="System Bold" size="13.0" />
                     </font></Label>
               </children>
            </HBox>
            <HBox layoutX="226.0" layoutY="148.0" prefHeight="38.0" prefWidth="200.0">
               <children>
                  <Label fx:id="Description" prefHeight="39.0" prefWidth="202.0" text="Description" />
               </children>
            </HBox>
            <HBox layoutX="223.0" layoutY="190.0" prefHeight="17.0" prefWidth="200.0">
               <children>
                  <Label fx:id="Avail" prefHeight="17.0" prefWidth="200.0" text="Avail" />
               </children>
            </HBox>
            <Label layoutX="146.0" layoutY="108.0" text="Condition: " />
            <Label layoutX="146.0" layoutY="131.0" text="Renter:" />
            <Label layoutX="146.0" layoutY="159.0" text="Description:" />
            <Label layoutX="146.0" layoutY="190.0" text="Availibility:" />
