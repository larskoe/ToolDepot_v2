            <TextArea editable="false" layoutX="53.0" layoutY="270.0" prefHeight="92.0" prefWidth="360.0" text="I agree to pay the price stated above and that if I, the rentee, damage the tool, I will buy or fixthe tool according to the renter." wrapText="true" />
            <Label layoutX="53.0" layoutY="257.0" text="Terms and Conditions">
               <font>
                  <Font size="10.0" />
               </font>
            </Label>
            <Button layoutX="209.0" layoutY="391.0" mnemonicParsing="false" onAction="#rentComplete" text="Rent" />
            <HBox alignment="CENTER" layoutX="7.0" layoutY="418.0" prefHeight="22.0" prefWidth="439.0">
               <children>
                  <Label fx:id="Status" alignment="CENTER" textFill="RED" />
               </children>
            </HBox>
         </children>
      </Pane>
   </children>
</AnchorPane>
